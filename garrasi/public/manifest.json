{
    "manifest_version": 2,
    "name": "Garrasi",
    "description": "Garrasi",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "cookies",
      "tabs"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.firebase.com https://*.firebaseio.com https://*.firebaseio.com; object-src 'self'; "
}

